<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="48dp"
    android:height="48dp"
    android:viewportWidth="800"
    android:viewportHeight="400">

    <!-- Main body of the brick -->
    <path
        android:fillColor="#8A8A8A"
        android:pathData="M50,100 h700 v200 h-700 z"/>

    <!-- Brick texture lines -->
    <path
        android:strokeColor="#6A6A6A"
        android:strokeWidth="10"
        android:pathData="M100,110 h600 M100,130 h600 M100,150 h600 M100,170 h600 M100,190 h600 M100,210 h600 M100,230 h600 M100,250 h600 M100,270 h600 M100,290 h600"/>

    <!-- Main divisions within the brick -->
    <path
        android:strokeColor="#4E4E4E"
        android:strokeWidth="5"
        android:pathData="M50,100 h700 M50,300 h700 M400,100 v200"/>

    <!-- Stronger outline for the entire brick -->
    <path
        android:strokeColor="#2A2A2A"
        android:strokeWidth="20"
        android:pathData="M50,100 h700 v200 h-700 z"/>
</vector>
